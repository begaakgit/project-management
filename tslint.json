{
    "extends": [],
    "linterOptions": {
        "exclude": [
            "config/**/*.js",
            "node_modules/**/*.ts",
            "coverage/lcov-report/*.js"
        ]
    },
    "rules": {
        "no-console": false,
        "no-require-imports": true,
        "ordered-imports": false,
        "no-any": true
    },
    "jsRules": {
        "no-console": false
    }
}